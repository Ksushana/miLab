extends ./layout/layout.pug

block globalVars

block headEnd

block bodyStart

block content
  .full-info.container
    .subtitle.notes
      a(href="./kits.html") ↲ Kits
    .full-info__grid
      .full-info__kit
        .kit.kit--no-btns
          .kit__header.kit__header--orange.kit__header--no-btns
            .kit__subtitle 24 reactions
            .kit__title.h2-kit Human multiplex TCR
          ul.kit__list
            li Multiplex PCR
            li TCR alpha &amp; beta
            li Blood / Cells / Tissue / FF / FFPE
            li 150 + 150 Sequencing
            li MiSeq / NextSeq / HiSeq / NovaSeq
            li High accuracy

      .full-info__info
        h1.full-info__title.title-md.h1  Human multiplex TCR Kit
        p.full-info__text.text
          | Allows synthesis of multiplex human TCR cDNA libraries suitable for sequencing on Illumina®
        .full-info__links
          button.bordered-link.bordered-link--black.inactive(type="button") coming soon
          div
            a(href="").bordered-link
              +svgicon('sidebar')
              span MixCR.Pro
            a(href="").bordered-link
              +svgicon('file-btn')
              span Kit manual
        .full-info__table
          .full-info__table-item
            .full-info__table-title.notes-title Species
            .full-info__table-data.notes
              span Human
          .full-info__table-item
            .full-info__table-title.notes-title Chains
            .full-info__table-data.notes
              span TCR alpha and beta
          .full-info__table-item
            .full-info__table-title.notes-title Material
            .full-info__table-data.notes
              span RNA
          .full-info__table-item
            .full-info__table-title.notes-title Sequencing
            .full-info__table-data.notes
              span 150 + 150  (recommended)
          .full-info__table-item
            .full-info__table-title.notes-title Software
            .full-info__table-data.notes
              span MiXCR Pro (included)

  .kits.kits--also.container
    .subtitle.notes
      span CHECK ALSO
    .kits__grid.kits__grid--scroll
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--pink
            .kit__subtitle 24 Reactions
            .kit__title.h2 Human TCR Kit
          ul.kit__list
            li UMI 5’Race
            li IG heavy &amp; light
            li Blood/cells/tissue/FF
            li 250+250 sequencing
            li Full length + isotypes
            li High accuracy
          .kit__btns
            button.kit__button(type="button").bordered-link.bordered-link--black Buy, $2000
            a.kit__button.notes.bordered-link(href="") Learn more ↳
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--blue
            .kit__subtitle 24 Reactions
            .kit__title.h2 Human 7 genes
          ul.kit__list
            li UMI 5’Race
            li IG heavy &amp; light
            li Blood/cells/tissue/FF
            li 250+250 sequencing
            li Full length + isotypes
            li High accuracy
          .kit__btns
            button.kit__button(type="button").bordered-link.bordered-link--black Buy, $2000
            a.kit__button(href="./human7genes.html").bordered-link Learn more ↳




block bodyEnd

