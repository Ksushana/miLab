extends ./layout/layout.pug

block globalVars
  - PAGE_TITLE = 'MiLaboratories — Human Multiplex BCR Kit'

block variables
  - let isBlue4 = true;

block headEnd

block bodyStart

block content
  .full-info.container
    .subtitle.notes
      a(href="./kits") ↲ Kits
    .full-info__grid
      .full-info__kit
        .kit.kit--no-btns
          .kit__header.kit__header--blue.kit__header--no-btns
            .kit__subtitle 192 reactions
            .kit__title.h2-kit Human multiplex BCR
          ul.kit__list
            li UMI multiplex
            li IG heavy
            li Blood / Cells / Tissue / FF
            li 250 + 250 Sequencing
            li Full length + isotypes
            li High accuracy

      .full-info__info
        h1.full-info__title.title-md.h1  Human multiplex BCR Kit
        p.full-info__text.text
          | Allows to obtain high quality full-length IGH repertoires. UMI-based analysis increases the quality of long reads and provides unprecedented accuracy of analysis, allowing to get the full length immunoglobulin repertoire with hypermutations and without errors, with accurate identification of immunoglobulin isotypes.
        .full-info__links
          button.bordered-link.bordered-link--black.inactive(type="button") coming soon
          div
            a(href="").bordered-link
              +svgicon('sidebar')
              span.no-uppercase MiXCR PRO
            a(href="").bordered-link
              +svgicon('file-btn')
              span Kit manual
        .full-info__table
          .full-info__table-item
            .full-info__table-title.notes-title Species
            .full-info__table-data.notes
              span Human
          .full-info__table-item
            .full-info__table-title.notes-title Chains
            .full-info__table-data.notes
              span IG heavy
          .full-info__table-item
            .full-info__table-title.notes-title Material
            .full-info__table-data.notes
              span.colored.color-tag-IgM IgM
              span.colored.color-tag-IgD IgD
              span.colored.color-tag-IgG1 IgG1
              span.colored.color-tag-IG2 IG2
              span.colored.color-tag-IgG3 IgG3
              span.colored.color-tag-IgG4 IgG4
              span.colored.color-tag-IgA IgA
              span.colored.color-tag-IgE IgE
          .full-info__table-item
            .full-info__table-title.notes-title Sequencing
            .full-info__table-data.notes
              span 250 + 250  (full length)
          .full-info__table-item
            .full-info__table-title.notes-title Software
            .full-info__table-data.notes
              span <span class="no-uppercase">MiXCR PRO</span> (included)

  .kits.kits--also.container
    .subtitle.notes
      span CHECK ALSO
    .kits__grid.kits__grid--scroll
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--orange
            .kit__subtitle 24 reactions
            .kit__title.h2-kit Human multiplex TCR
          ul.kit__list
            li Multiplex PCR
            li TCR alpha &amp; beta
            li Blood / Cells / Tissue / FF / FFPE
            li 150 + 150 Sequencing
            li MiSeq / NextSeq / HiSeq / NovaSeq
            li High accuracy
          .kit__btns
            button.kit__button.notes.bordered-link.bordered-link--black.inactive(type="button") coming soon
            a.kit__button.notes.bordered-link(href="./humanmultiTCRkit") Learn more ↳
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--green
            .kit__subtitle 24 reactions
            .kit__title.h2-kit Monkey 5’RACE TCR
          ul.kit__list
            li UMI 5’RACE
            li TCR alpha &amp; beta
            li Blood / Cells / Tissue / FF
            li 150 + 150 nt Sequencing
            li MiSeq/NextSeq/HiSeq/NovaSeq
            li High accuracy
          .kit__btns
            button.kit__button.notes.bordered-link.bordered-link--black.inquire-button(type="button", data-subject="Monkey 5’RACE TCR Kit") Inquire
            a.kit__button.notes.bordered-link(href="./monkeyTCRkit") Learn more ↳

      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--pink
            .kit__subtitle 192 reactions
            .kit__title.h2-kit HUMAN TCR
          ul.kit__list
            li UMI 5’RACE
            li TCR ALPHA &amp; BETA
            li BLOOD / CELLS / TISSUE
            li 250 + 250 Sequencing
            li MISEQ / NEXTSEQ
            li High accuracy
          .kit__btns
            button.kit__button.notes.bordered-link.bordered-link--black.inquire-button(type="button", data-subject="Human TCR Kit") Inquire
            a.kit__button.notes.bordered-link(href="./humantcrkit") Learn more ↳
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--purple
            .kit__subtitle 24 reactions
            .kit__title.h2-kit HUMAN DNA TCR
          ul.kit__list
            li DNA MULTIPLEX
            li TCR ALPHA &amp; BETA
            li BLOOD / CELLS / TISSUE / FF / FFPE
            li 150 + 150 NT SEQUENCING
            li HIGH ACCURACY
            li MISEQ/NEXTSEQ/HISEQ/NOVASEQ
          .kit__btns
            button.kit__button.notes.bordered-link.bordered-link--black.inquire-button(type="button", data-subject="Human DNA TCR Kit") Inquire
            a.kit__button.notes.bordered-link(href="./humanDNAkit") Learn more ↳




block bodyEnd

block bodyVendorScripts

block bodyCustomScripts
