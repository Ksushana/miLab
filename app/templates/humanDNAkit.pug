extends ./layout/layout.pug

block globalVars

block variables
  - let isPink = true;

block headEnd

block bodyStart

block content
  .full-info.container
    .subtitle.notes
      a(href="./kits.html") ↲ Kits
    .full-info__grid
      .full-info__kit
        .kit.kit--no-btns
          .kit__header.kit__header--purple.kit__header--no-btns
            .kit__subtitle 24 reactions
            .kit__title.h2-kit Human DNA TCR
          ul.kit__list
            li DNA multiplex
            li TCR alpha &amp; beta
            li Blood / Cells / Tissue / FF / FFPE
            li 150 + 150 nt Sequencing
            li High accuracy
            li MiSeq/NextSeq/HiSeq/NovaSeq

      .full-info__info
        h1.full-info__title.title-md.h1  Human DNA TCR Kit
        p.full-info__text.text
          | Allows to obtain high quality TCR alpha and beta repertoires for different types of available DNA material, with the highest possible sensitivity.
        .full-info__links
          button.bordered-link.bordered-link--black.inactive(type="button") coming soon
          div
            a(href="").bordered-link
              +svgicon('sidebar')
              span MiXCR PRO
            a(href="").bordered-link
              +svgicon('file-btn')
              span Kit manual
        .full-info__table
          .full-info__table-item
            .full-info__table-title.notes-title Species
            .full-info__table-data.notes
              span Human
          .full-info__table-item
            .full-info__table-title.notes-title Chains
            .full-info__table-data.notes
              span TCR alpha and beta
          .full-info__table-item
            .full-info__table-title.notes-title Material
            .full-info__table-data.notes
              span DNA
          .full-info__table-item
            .full-info__table-title.notes-title Sequencing
            .full-info__table-data.notes
              span 150 + 150  (recommended)
          .full-info__table-item
            .full-info__table-title.notes-title Software
            .full-info__table-data.notes
              span MiXCR Pro (included)

  .kits.kits--also.container
    .subtitle.notes
      span CHECK ALSO
    .kits__grid.kits__grid--scroll
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--orange
            .kit__subtitle 24 reactions
            .kit__title.h2-kit Human multiplex TCR
          ul.kit__list
            li Multiplex PCR
            li TCR alpha &amp; beta
            li Blood / Cells / Tissue / FF / FFPE
            li 150 + 150 Sequencing
            li MiSeq / NextSeq / HiSeq / NovaSeq
            li High accuracy
          .kit__btns
            button.kit__button.notes.bordered-link.bordered-link--black.inactive(type="button") coming soon
            a.kit__button.notes.bordered-link(href="./humanmultitcrkit.html") Learn more ↳
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--green
            .kit__subtitle 24 reactions
            .kit__title.h2-kit Monkey 5’RACE TCR
          ul.kit__list
            li UMI 5’RACE
            li TCR alpha &amp; beta
            li Blood / Cells / Tissue / FF
            li 150 + 150 nt Sequencing
            li MiSeq/NextSeq/HiSeq/NovaSeq
            li High accuracy
          .kit__btns
            button.kit__button.notes.bordered-link.bordered-link--black(type="button") Buy, $2000
            a.kit__button.notes.bordered-link(href="./monkeytcrkit.html") Learn more ↳
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--blue
            .kit__subtitle 192 reactions
            .kit__title.h2-kit Human multiplex BCR
          ul.kit__list
            li UMI multiplex
            li IG heavy
            li Blood / Cells / Tissue / FF
            li 250 + 250 Sequencing
            li Full length + isotypes
            li High accuracy
          .kit__btns
            button.kit__button.notes.bordered-link.bordered-link--black.inactive(type="button") coming soon
            a.kit__button.notes.bordered-link(href="./humanmultibcrkit.html") Learn more ↳
      .kits__grid-item
        .kit.kit--scroll
          .kit__header.kit__header--pink
            .kit__subtitle 192 reactions
            .kit__title.h2-kit HUMAN TCR
          ul.kit__list
            li UMI 5’RACE
            li TCR ALPHA &amp; BETA
            li BLOOD / CELLS / TISSUE
            li 250 + 250 Sequencing
            li MISEQ / NEXTSEQ
            li High accuracy
          .kit__btns
            button.kit__button.notes.bordered-link.bordered-link--black(type="button") Buy, $2000
            a.kit__button.notes.bordered-link(href="./humantcrkit.html") Learn more ↳



block bodyEnd

